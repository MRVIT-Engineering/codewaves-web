{
  "name": "codewaves-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start && yarn run lint",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --env=jsdom --testPathPattern=unit",
    "test:e2e": "react-scripts test --testPathPattern=e2e --testPathIgnorePatterns=e2e/constants",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "init-husky": "npx husky-init && yarn install",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 110,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.2",
    "@mui/material": "^5.0.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@tinymce/tinymce-react": "^3.12.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-animate-on-scroll": "^2.1.2",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@uiw/react-codemirror": "^3.0.10",
    "animate.css": "^4.1.1",
    "auto-bind": "^4.0.0",
    "axios": "^0.21.1",
    "bootstrap": "5.1.1",
    "codemirror-minimap": "^1.0.6",
    "devicon-react-svg": "^0.2.3",
    "hamburger-react": "^2.4.1",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "notistack": "^1.0.10",
    "react": "^17.0.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-bootstrap": "^2.0.0-rc.0",
    "react-devicon": "^0.1.9",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-notifications-component": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-simple-typewriter": "^1.0.13",
    "react-spinners": "^0.11.0",
    "socket.io-client": "^4.1.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@types/react-notifications-component": "^3.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  }
}
